keywords = {Animals,Hippocampus,Hippocampus: physiology,Humans,Learning,Learning: physiology,Long-Term Potentiation,Long-Term Potentiation: physiology,Memory,Memory: physiology,Models, Neurological,Nerve Net,Nerve Net: physiology,Neuronal Plasticity,Neuronal Plasticity: physiology,Synapses,Synapses: physiology},
