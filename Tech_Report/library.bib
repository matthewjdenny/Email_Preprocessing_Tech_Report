@article{Yano2012,
abstract = {A U. S. Congressional bill is a textual artifact that must pass through$\backslash$na series of hurdles to become a law. In this paper, we focus on one$\backslash$nof the most precarious and least understood stages in a bill's life:$\backslash$nits consideration, behind closed doors, by a Congressional committee.$\backslash$nWe construct predictive models of whether a bill will survive committee,$\backslash$nstarting with a strong, novel baseline that uses features of the$\backslash$nbill's sponsor and the committee it is referred to. We augment the$\backslash$nmodel with information from the contents of bills, comparing different$\backslash$nhypotheses about how a committee decides a bill's fate. These models$\backslash$ngive significant reductions in prediction error and highlight the$\backslash$nimportance of bill substance in explanations of policy-making and$\backslash$nagenda-setting.},
author = {Yano, Tae and Smith, Noah a and Wilkerson, John D},
