journal = {Proceedings of the National Academy of Sciences of the United States of America},
month = jun,
number = {23},
pages = {8577--82},
pmid = {16723398},
title = {{Modularity and community structure in networks.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1482622\&tool=pmcentrez\&rendertype=abstract},
